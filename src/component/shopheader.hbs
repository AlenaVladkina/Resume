<div class="row">
    {{#with navigation}}
    <div class="col-6">
        <h1>Shop</h1>
    </div>
    <div class="col-6">
        <div class="nav">
            {{#each links}}
            <div class="nav-item">
                <a class="nav-link {{#if @last}}disabled{{/if}}" href="{{href}}" >{{text}}</a>
            </div>
            {{/each}}
            <div class="nav-item">
            </div>
        </div>
    </div>

   
   