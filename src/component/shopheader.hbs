<div class="row">
    {{#with navigation}}
    <div class="col-6">
        <h1>Shop</h1>
    </div>
    <div class="col-6">
        <div class="nav">
            {{#each links}}

            <div>
                <a class="nav-link
                    {{#if @last}} disabled {{/if}}" href="{{link}}">{{text}}</a>
            </div>
            {{/each}}




        </div>
    </div>
    {{/with}}
</div>