<main>
    <div>
        {{#with navigation}}
        {{#each links}}
        <button class="btn btn-link" href="{{href}}">{{text}}</button>
        {{/each}}
        <button class="btn btn-primary" href="{{button.href}}">{{button.text}}</button>
        {{/with}}
    </div>

    <hr>

    <div>
        {{#with header}}
        <h1>{{title}}</h1>
        <p>{{description}}</p>
        <button class="btn btn-primary" href="{{button.link}}">
            {{button.text}}</button>
        {{/with}}
    </div>

    <hr>

    <div>
        {{#with city}}
        <h2>{{title}}</h2>
        <div class="row g-3">
            {{#each cards}}
            <div class="col-6">
                <div class="card">
                    <img src="{{picture.src}}" class="card-img-top" alt="{{picture.alt}}">
                    <div class="card-body">
                        <h5 class="card-title">{{title}}</h5>
                        <p class="card-text">{{description}}</p>
                        <a href="{{button.href}}" class="btn btn-primary">
                            {{button.text}}</a>
                    </div>
                </div>
            </div>
            {{/each}}
        </div>
        {{/with}}
    </div>

    <hr>

    <div>
        {{#with winner}}
        <h2>{{ title}}</h2>
        {{#with table}}
        <table class="table">
            <thead>
                <tr>
                    <th>{{head.[0]}}</th>
                    <th>{{head.[1]}}</th>
                    <th>{{head.[2]}}</th>
                    <th>{{head.[3]}}</th>
                </tr>
            </thead>
            {{#each user}}
            <tbody>
                <tr>
                    <td>{{place}} </td>
                    <td>{{name}}</td>
                    <td>{{surname}}</td>
                    <td>{{nickname}}</td>
                </tr>
            </tbody>
            {{/each}}
            {{/with}}
        </table>
        {{/with }}

        <hr>

        {{#with gallery}}
        <h2>{{title}}</h2>
        <br>
        <div class="row g-3">
            {{#each cards}}
            <div class="col-4">
                <div class="list-group">
                    <div class="list-group-item text-bg-secondary">{{name}}</div>
                    <div class="list-group-item">{{items}}</div>


                </div>
            </div>
            {{/each}}
        </div>
        {{/with}}

        <hr>

        <div>
            {{#with relaxion}}
            <div class="display-3">{{title}}</div>
            <br>
            <div class="row g-3">
                {{#each cards}}
                <div class="col-6">
                    <div class="card text-bg-secondary">
                        <div class="card-header"> {{header}}</div>
                        <div class="card-body">
                            <h5 class="card-title">{{title}}</h5>
                            <p class="card-text">{{description}}</p>
                        </div>
                    </div>
                </div>
                {{/each}}
            </div>
            {{/with}}
        </div>

        <hr>

        <div>
            {{#with rest}}
            <div class="row g-3">
                {{#each cards}}
                <div class="col-6">
                    <div class="card text-center">
                        <div class="card-header"> {{header}}</div>
                        <div class="card-body">
                            <h5 class="card-title">{{title}}</h5>
                            <p class="card-text">{{description}}</p>
                            <button class="btn btn-primary" href="#">{{button}}</button>
                        </div>
                        <div class="card-footer">{{footer}}</div>
                    </div>
                </div>
                {{/each}}
            </div>

            {{/with}}
        </div>

        <hr>

        <div>
            {{#with proposition}}
            <div class="display-2">{{title}}</div>
            <br>
            <div class="row g-3">
                {{#each card}}
                <div class="col-6">
                    <div class="card text-bg-warning">
                        <div class="card-header"> {{header}}</div>
                        <div class="card-body">
                            <h5 class="card-title">{{title}}</h5>
                            <p class="card-text">{{description}}</p>
                        </div>
                    </div>
                </div>
                {{/each}}
            </div>
            {{/with}}
        </div>

        <hr>

        <div>
            {{#with footer}}
            <h5>{{title}}</h5>

            <div class="row row-cols-10 row-cols-md-3 g-10">
                <div class="nav flex-column">

                    {{#each navigation}}
                    <ul>
                        <button class="btn btn-link" href="{{href}}">{{text}}</button>
                    </ul>
                    {{/each}}
                </div>
                <div>
                    {{#each socialMedia}}
                    <ul>
                        <button class="btn btn-outline text-dark" href="{{link}}">{{name}}</button>
                    </ul>
                    {{/each}}
                </div>
                <div>
                    {{#each partners}}
                    <ul>
                        <button class="btn btn-outline text-info" href="{{link}}">{{name}}</button>
                    </ul>
                    {{/each}}
                </div>
            </div>

        </div>
        {{/with}}
    </div>
    </div>
</main>