<div>
    <section>
        {{#with person}}
        <div>
            <h1>{{name}}</h1>
        </div>
        <div>
            <p>age: "{{age}}"</p>
            <p>gender: "{{gender}}"</p>
            <hr>
            <address>Address:
                {{#with address}}
                <p>{{street}}</p>
                <p>{{city}}</p>
                <p>{{state}}</p>
                <p>{{zip}}</p>
                <p>{{country}}</p>
                {{/with}}
            </address>
        </div>
        {{/with}}
</div>
</section>
<hr>
<section>
    <div>
        <h2>Education</h2>
    </div>
    <div>
        {{#with person}}
        {{#each education}}
        <p>Degree: {{degree}}</p>
        <p>Major: {{major}}</p>
        <p>University: {{university}}</p>
        <p>Graduation year: {{graduationYear}}</p>
        {{/each}}
        {{/with}}
    </div>
</section>
<section>
    <div>
        <hr>
        <h2>Work experience</h2>
    </div>
    <div>
        {{#with person}}
        {{#each workExperience}}
        <p>Company: {{company}}</p>
        <p>Title: {{title}}</p>
        <p>Start date: {{startDate}}</p>
        <p>End date: {{endDate}}</p>
        <p>Responsibilities: {{responsibilities}}</p>
        <p>Year founded: {{year_founded}}</p>
        <p>Industry: {{industry}}</p>
        {{#each employees}}
        <p>Employees: {{name}}</p>
        <p>Position {{position}}</p>
        <p>Department: {{department}}</p>
        {{#each projects}}
        <p>Projects: {{name}}</p>
        <p>Description: {{description}}</p>
        <p>Status: {{status}}</p>
        {{#each teams}}
        <p>Teams: {{team_name}} </p>
        {{#with team_leader}}
        <p>Team leader: {{name}}</p>
        <p>Title: {{title}}</p>
        <p>Email: {{email}}</p>
        {{/with}}
        {{#each team_members}}
        <p>Team members: </p>
        <ul>
            <li>
                <p>{{name}}</p>
                <p>{{title}}</p>
                <p>{{email}}</p>
                <p>{{skills}}</p>
                {{#each projects}}
                <p>Projects: {{name}}</p>
                <p>Description: {{description}}</p>
                <p>Technologies: {{technologies}}</p>
            </li>
            {{#each team_members}}
            <li>
                {{name}}
                {{title}}
            </li>
            {{/each}}
            {{/each}}
        </ul>
        {{/each}}
        {{/each}}
        {{/each}}
        {{/each}}
        {{/each}}
        {{/with}}
    </div>
</section>